<h1>Appointments#new</h1>
<p>Find me in app/views/appointments/new.html.erb</p>

<%= form_for @appointment do |f| %>
<label>Appt Slot</label>
<%= f.text_field :appt_slot %>
<br>
<label>Select Doctor</label>
<%= f.collection_select :doctor_id, Doctor.all, :id, :name %>
<br>
<label>Select Patient</label>
<%= f.collection_select :patient_id, Patient.all, :id, :name %>
<br>
<%= f.submit %>
<% end %>